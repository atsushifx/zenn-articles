# EditorConfig is awesome: https://EditorConfig.org
#
# default editorconfig set ~/ to symlink

# top-most EditorConfig file
# root = true


### json]
[*.json]
tab_width = 4
indent_style = tab

### rc files
[*rc]
tab_width = 4
indent_style = tab

### markdown
[*.md]
tab_width = 2
indent_style = space

